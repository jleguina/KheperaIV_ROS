<?xml version="1.0" ?>
<robot name="kh4_robot">
 
 	<!-- Chassis -->
 	
  <link name="base_link">
    <collision name="chassis_collision">
      <!--<origin xyz="0 0 0.0312" rpy="0 0 0"/>-->
      <geometry>
        <cylinder length=".053" radius=".0704"/>
      </geometry>
    </collision>
    
    <visual name="chassis_visual">
      <!--<origin xyz="0 0 0.0312" rpy="0 0 0"/>-->
      <geometry>
        <cylinder length=".053" radius=".0704"/>
      </geometry>
    </visual>
    
    <collision name="chassis_front_caster_collision">
      <origin xyz="0.05 0 -0.0265" rpy="0 0 0"/>
      <geometry>
        <sphere radius=".0047"/>
      </geometry>
    </collision>
    
    <visual name="chassis_front_caster_visual">
      <origin xyz="0.05 0 -0.0265" rpy="0 0 0"/>
      <geometry>
        <sphere radius=".0047"/>
      </geometry>
    </visual>
    
    <collision name="chassis_back_caster_collision">
      <origin xyz="-0.05 0 -0.0265" rpy="0  0  0"/>
      <geometry>
        <sphere radius=".0047"/>
      </geometry>
    </collision>
    
    <visual name="chassis_back_caster_visual">
      <origin xyz="-0.05 0 -0.0265" rpy="0 0 0"/>
      <geometry>
        <sphere radius=".0047"/>
      </geometry>
    </visual>
  </link>
  
  <!-- Dummy Chassis -->

  <link name="chassis_inertia">
    <inertial>
      <mass value="0.5"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="0.000834" ixy="0" ixz="0" iyy="0.000834" iyz="0" izz="0.001403"/>
    </inertial>  	
  </link>
 
  <joint name="chassis_inertia_joint" type="fixed">
    <parent link="base_link"/>
    <child link="chassis_inertia"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 0"/>
  </joint>  
  
	<!-- Left Wheel -->
  
  <joint name="left_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="left_wheel"/>
    <origin xyz="0 0.0527 -0.0102" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>  
  
  <link name="left_wheel">
    <inertial>
      <mass value="0.033"/>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <inertia ixx="0.000004" ixy="0" ixz="0" iyy="0.000004" iyz="0" izz="0.000007"/>
    </inertial>
    
    <collision name="left_wheel_collision">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder length="0.005" radius="0.021"/>
      </geometry>
    </collision>
    
    <visual name="left_wheel_visual">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder length="0.005" radius="0.021"/>
      </geometry>
    </visual>
  </link>
   
  
 	<!-- Right Wheel -->
  
  <joint name="right_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="right_wheel"/>
    <origin xyz="0 -0.0527 -0.0102" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>  
  
  <link name="right_wheel">
    <inertial>
      <mass value="0.033"/>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <inertia ixx="0.000004" ixy="0" ixz="0" iyy="0.000004" iyz="0" izz="0.000007"/>
    </inertial>
    
    <collision name="right_wheel_collision">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder length="0.005" radius="0.021"/>
      </geometry>
    </collision>
    
    <visual name="right_wheel_visual">
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/>
      <geometry>
        <cylinder length="0.005" radius="0.021"/>
      </geometry>
    </visual>
  </link>
   
  
  <!-- ROS control -->
  
  <gazebo>
    <plugin name="gazebo_ros_control" filename="libgazebo_ros_control.so">
    	<robotParam>/robot_description</robotParam>
    </plugin>
  </gazebo>  
  
<!--
  <gazebo>
  	<plugin name="differential_drive_controller" filename="libgazebo_ros_diff_drive.so">
  	  <alwaysOn>true</alwaysOn>
  	  <updateRate>40</updateRate>
  	  <leftJoint>left_wheel_joint</leftJoint>
  	  <rightJoint>right_wheel_joint</rightJoint>
  	  <wheelSeparation>0.1054</wheelSeparation>
  	  <wheelDiameter>0.042</wheelDiameter>
  	  <torque>20</torque>
  	  <commandTopic>cmd_vel</commandTopic>
  	  <odometryTopic>odom</odometryTopic>
  	  <odometryFrame>odom</odometryFrame>
  	  <robotBaseFrame>base_link</robotBaseFrame>
  	</plugin>
	</gazebo>
-->
  
  <!-- Wheel transmissions -->
  
  <transmission name="left_wheel_transmission">
  	<type>transmission_interface/SimpleTransmission</type>
  	<joint name="left_wheel_joint">
    	<hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
  	</joint>
  	<actuator name="left_wheel_motor">
    	<mechanicalReduction>1</mechanicalReduction>
    	<hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
  	</actuator>
	</transmission>
	
  <transmission name="right_wheel_transmission">
  	<type>transmission_interface/SimpleTransmission</type>
  	<joint name="right_wheel_joint">
    	<hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
  	</joint>
  	<actuator name="right_wheel_motor">
  		<mechanicalReduction>1</mechanicalReduction>
    	<hardwareInterface>hardware_interface/VelocityJointInterface</hardwareInterface>
  	</actuator>
	</transmission>	

	<!-- Wheel gazebo parameters -->

  <gazebo reference="left_wheel">
  	<mu1 value="200.0"/>
    <mu2 value="100.0"/>
    <kp value="10000000.0" />
    <kd value="1.0" />
  </gazebo>

  <gazebo reference="right_wheel">
  	<mu1 value="200.0"/>
    <mu2 value="100.0"/>
    <kp value="10000000.0" />
    <kd value="1.0" />
  </gazebo>

	<!-- IR Sensor -->
	
  <joint name="IR_joint" type="fixed">
    <axis xyz="0 0 1" />
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="IR_link"/>
  </joint>

  <link name="IR_link">
    <inertial>
      <mass value="1e-5" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial> 
  </link>	

  <gazebo reference="IR_link">
    <sensor type="ray" name="ray_sensor">
      <pose>0 0 0 0 0 0</pose>
      <visualize>0</visualize>
      <update_rate>40</update_rate>
      <ray>
        <scan>
      		<horizontal>
       			<samples>8</samples>
       			<resolution>1</resolution>
       			<min_angle>0</min_angle>
       			<max_angle>5.49778</max_angle>
       		</horizontal>
       	</scan>
       	<range>
       		<min>0.0705</min> <!-- Already added the Kh4 radius -->
       		<max>0.3204</max> <!-- Already added the Kh4 radius -->
       	</range>
        <noise>
          <type>gaussian</type>
          <!-- Noise parameters must be base based on published spec for Vishay Telefunken TCRT5000 -->
          <mean>0.0</mean>
          <stddev>0.01</stddev>
        </noise>
      </ray>
      <plugin name="gazebo_ros_head_hokuyo_controller" filename="libgazebo_ros_laser.so">
        <topicName>/sensor_msgs/LaserScan</topicName>
        <frameName>IR_link</frameName>
      </plugin>
    </sensor>
  </gazebo>
	
	<!-- IMU Sensor -->

  <joint name="imu_joint" type="fixed">
    <axis xyz="0 0 1" />
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="imu_link"/>
  </joint>

  <link name="imu_link">
    <inertial>
      <mass value="1e-5" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial>
  </link>	


  <gazebo reference="imu_link">
    <gravity>true</gravity>
    <sensor name="imu_sensor" type="imu">
      <always_on>true</always_on>
      <update_rate>100</update_rate>
      <visualize>0</visualize>
      <topic>__default_topic__</topic>
      <plugin filename="libgazebo_ros_imu_sensor.so" name="imu_plugin">
        <topicName>/sensor_msgs/imu</topicName>
        <bodyName>imu_link</bodyName>
        <updateRateHZ>10.0</updateRateHZ>
        <gaussianNoise>0.0</gaussianNoise>
        <xyzOffset>0 0 0</xyzOffset>
        <rpyOffset>0 0 0</rpyOffset>
        <frameName>imu_link</frameName>
      </plugin>
      <pose>0 0 0 0 0 0</pose>
    </sensor>
  </gazebo>

	<!-- Sonar Sensor -->
	
  <joint name="sonar_joint" type="fixed">
    <axis xyz="0 0 1" />
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="sonar_link"/>
  </joint>

  <link name="sonar_link">
    <inertial>
      <mass value="1e-5" />
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
    </inertial> 
  </link>	

  <gazebo reference="sonar_link">
    <sensor type="ray" name="sonar_sensor">
      <pose>0 0 0 0 0 0</pose>
      <visualize>1</visualize>
      <update_rate>40</update_rate>
      <ray>
        <scan>
      		<horizontal>
       			<samples>5</samples>
       			<resolution>1</resolution>
       			<min_angle>-1.5707963</min_angle>
       			<max_angle>1.5707963</max_angle>
       		</horizontal>
       	</scan>
       	<range>
       		<min>0.3204</min> <!-- Already added the Kh4 radius -->
       		<max>2.0704</max> <!-- Already added the Kh4 radius -->
       	</range>
        <noise>
          <type>gaussian</type>
          <!-- Noise parameters must be base based on published spec for Vishay Telefunken TCRT5000 -->
          <mean>0.0</mean>
          <stddev>0.01</stddev>
        </noise>
      </ray>
      <plugin name="gazebo_ros_head_hokuyo_controller" filename="libgazebo_ros_laser.so">
        <topicName>/sensor_msgs/SonarScan</topicName>
        <frameName>sonar_link</frameName>
      </plugin>
    </sensor>
  </gazebo>


</robot>
